{
    "name": "dalley-it/contao-photoswipe",
    "description": "Contao modul to easily handle the javascript library photoswipe",
    "homepage": "https://www.dalley-it.com/blog/contao-photoswipe.html",
    "support": {
        "issues": "https://github.com/dalley-it/contao-photoswipe/issues",
        "source": "https://github.com/dalley-it/contao-photoswipe"
    },
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Dalley IT - Oliver Dalley",
            "homepage": "https://www.dalley-it.com",
            "role": "Developer"
        }
    ],
    "keywords": [
        "Contao",
        "photoswipe",
        "swipe",
        "gallery"
    ],
    "type": "contao-bundle",
    "require": {
        "php": "^7.4 || ^8.0",
        "contao/core-bundle": "^4.13 || ^5.3"
    },
    "conflict": {
      "contao/core": "*"
    },
    "extra": {
        "contao-manager-plugin": "Dait\\DalleyItPhotoSwipe\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Dait\\DalleyItPhotoSwipe\\": "src/"
        },
        "exclude-from-classmap": [
          "contao/config/",
          "contao/dca/",
          "contao/languages/",
          "contao/templates/"
        ]
    }
}
